
service:
  name: "service.{{ name }}"
  url: "{{ url }}"
  protocol: "{{ protocol|default('http') }}"
  host: "{{ host }}"
  port: {{ port|default(80) }}
  path: "{{ path|default('/') }}"
  retries: {{ retries|default(5) }}
  connect_timeout: {{ connect_timeout|default(60000) }}
  write_timeout: {{ write_timeout|default(60000) }}
  read_timeout: {{ read_timeout|default(60000) }}
  tags: [{% for tag in tags %}"{{ tag }}"{% if not loop.last %}, {% endif %}{% endfor %}]
